{
  "name": "11ty5",
  "version": "0.0.1",
  "description": "A 5tarter for Eleventy/Tailwind based projects",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:11ty": "eleventy --quiet",
    "build:rollup": "rollup -c",
    "clean": "rm -rf dist",
    "dev": "npm-run-all clean build:* -p dev:*",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:rollup": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndiKod/11ty5"
  },
  "author": {
    "name": "Andrei Curelaru",
    "email": "andrei@andikod.fr"
  },
  "license": "MIT",
  "homepage": "https://11ty5.netlify.app",
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@rollup/plugin-replace": "^2.4.2",
    "@tailwindcss/forms": "0.3.2",
    "autoprefixer": "^10.2.4",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "2.45.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "2.1.1"
  },
  "dependencies": {
    "@11ty/eleventy-cache-assets": "^2.1.0",
    "@11ty/eleventy-img": "^0.8.3",
    "axios": "^0.21.1"
  }
}
